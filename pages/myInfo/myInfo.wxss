.login-logo {
  width: 100%;
  padding: 120rpx 0;
  text-align: center;
}

.login-logo-img {
  width: 380rpx;
  height: 100rpx;
}

.login-input {
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 50rpx;
  text-align: center;
}

.login {
  width: 630rpx;
  height: 64rpx;
  background-color: rgb(250, 255, 189);
}

.login-btn-box {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 0 60rpx;
  margin-top: 100rpx;
  box-sizing: border-box;
  width: 100%;
}
.login-btn{
  font-size: 24rpx;
}
.btn1{
  margin-left: 30rpx;
}
.btn2{
  margin-right: 30rpx;
  color: red;
}
.but{
  width: 230rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 36rpx;
  background-color: #ccc;
  color: #fff;
  font-weight: 900;
}
.butRight{
  background-color: #fd5959;
  color: #fff;
}